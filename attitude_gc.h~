struct gc_attitude_struct 
{
	float kR11;
	float kR22;
	float kR33;
	float kOmega11;
	float kOmega22;
	float kOmega33;
};

void gc_attitude_Init(gc_attitude_struct *gc, float kR11, float kR22, float kR33, float kOmega11, float kOmega22, float kOmega33);
float* gc_attitude_Cal(gc_attitude_struct *gc, float eR1, float eR2, float eR3, float eOmega1, float eOmega2, float eOmega3);



